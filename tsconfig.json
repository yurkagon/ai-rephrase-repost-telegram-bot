{
    "compilerOptions": {
      "lib": ["ES2022"],
      "module": "node16",
      "target": "es2022",

      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16",
      "noEmitOnError": false
    }
  }
